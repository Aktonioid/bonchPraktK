@page "/GeoObjectDelete"

@using System.Net.Http
@using System.Net.Http.Json
@using System.Threading.Tasks
<h3>Удаление</h3>
<form asp-page-handler="OnDelete">
    <label >Код объекта</label><br>
    <input type="text" name="code" @bind="_code"><br>
    <br>
    <button class="btn" type="submit" onclick="@OnDelete">Удалить</button>
</form>
@code
{
    string _code = "code";
    HttpClient client = new();
    public async Task OnDelete()
    {
        using var response = await client.DeleteAsync($"https://localhost:8001/api/GeoObjectType/{_code}");
    }
}
